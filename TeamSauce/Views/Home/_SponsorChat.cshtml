@model dynamic

<h2>SPONSOR CHAT</h2>

<div id="sponsor-feed-holder" style="overflow:scroll;min-height: 250px; max-height: 250px; overflow-x: hidden">
    <ul id="sponsor-feed" class="chatbox"></ul>
</div>
<div class="chat-message-wrapper">
     <textarea id="sponsor-message" class="chat-message atwho"></textarea>
</div>
<input type="submit" id="send-sponsor-message" value="Send" class="btn btn-info btn-block" />
