@model dynamic

<div class="modal fade" role="dialog" id="questionnaireModal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Question time!</h4>
            </div>
            <div id="questions" class="modal-body" />
        </div>
        <div class="modal-footer">
            <button id="submitQuestionnaire" type="button" class="btn btn-primary" data-dismiss="modal">Go!</button>
        </div>
    </div>
</div>

